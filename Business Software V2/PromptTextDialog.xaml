<Window x:Class="Business_Software_V2.PromptTextDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Business_Software_V2"
        mc:Ignorable="d"
        Title="PromptTextDialog" Height="auto" Width="300"
        >
    <StackPanel>
        <Label FontSize="12pt" x:Name="CaptionLabel">Can't find ABN, please specify.</Label>
        <TextBox Height="30" FontSize="12pt" x:Name="text" PreviewKeyDown="text_PreviewKeyDown"/>
    </StackPanel>
</Window>
